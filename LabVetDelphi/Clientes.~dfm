  object UDPQualquer: TUpdateSQL
    ModifySQL.Strings = (
      'update Cliente'
      'set'
      '  Nome = :Nome,'
      '  Endereco = :Endereco,'
      '  numero = :numero,'
      '  Compl = :Compl,'
      '  Bairro = :Bairro,'
      '  CEP = :CEP,'
      '  Cidade = :Cidade,'
      '  UF = :UF,'
      '  Telefone = :Telefone,'
      '  Fax = :Fax,'
      '  Celular = :Celular,'
      '  email = :email,'
      '  cpf_cgc = :cpf_cgc,'
      '  Contato = :Contato,'
      '  DataCadastro = :DataCadastro,'
      '  Restricao = :Restricao,'
      '  DataAtualizacao = :DataAtualizacao,'
      '  Ativo = :Ativo,'
      '  CodVetResp = :CodVetResp'
      'where'
      '  CodCliente = :OLD_CodCliente')
    InsertSQL.Strings = (
      'insert into Cliente'
      
        '  (Nome, Endereco, numero, Compl, Bairro, CEP, Cidade, UF, Telef' +
        'one, Fax, '
      
        '   Celular, email, cpf_cgc, Contato, DataCadastro, Restricao, Da' +
        'taAtualizacao, '
      '   Ativo, CodVetResp)'
      'values'
      
        '  (:Nome, :Endereco, :numero, :Compl, :Bairro, :CEP, :Cidade, :U' +
        'F, :Telefone, '
      
        '   :Fax, :Celular, :email, :cpf_cgc, :Contato, :DataCadastro, :R' +
        'estricao, '
      '   :DataAtualizacao, :Ativo, :CodVetResp)')
    DeleteSQL.Strings = (
      'delete from Cliente'
      'where'
      '  CodCliente = :OLD_CodCliente')
    Left = 389
    Top = 165
  end
