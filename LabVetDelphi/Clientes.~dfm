      FixedChar = True
      Size = 40
    end
    object QryQualquernumero: TStringField
      FieldName = 'numero'
      Origin = 'DNLABVET.Cliente.numero'
      FixedChar = True
      Size = 50
    end
    object QryQualquerCompl: TStringField
