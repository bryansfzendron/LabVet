  object QrySaldoEntrada: TQuery
    DatabaseName = 'DNLABVET'
    SQL.Strings = (
      'SELECT e.CODBANCO,  b.NomeBanco, b.numconta,'
      'SUM(e.recebido) as VLRecebido'
      ''
      'FROM         V_SALDOENTRADA e'
      
        '                    inner join banco b on e.codbanco = b.codbanc' +
        'o'
      'WHERE     '
      'e.dtpgto < :dt'
      'group by e.CODBANCO, b.NomeBanco, b.numconta  ')
    Left = 88
    Top = 8
    ParamData = <
