  object UpdateSQL1: TUpdateSQL
    ModifySQL.Strings = (
      'update Saldo'
      'set'
      '  CODBANCO = :CODBANCO,'
      '  ENTRADA = :ENTRADA,'
      '  SAIDA = :SAIDA,'
      '  RECEBIDO = :RECEBIDO,'
      '  PAGO = :PAGO,'
      '  NOMEBANCO = :NOMEBANCO,'
      '  NUMCONTA = :NUMCONTA'
      'where'
      '  CODBANCO = :OLD_CODBANCO')
    InsertSQL.Strings = (
      'insert into Saldo'
      
        '  (CODBANCO, ENTRADA, SAIDA, RECEBIDO, PAGO, NOMEBANCO, NUMCONTA' +
        ')'
      'values'
      
        '  (:CODBANCO, :ENTRADA, :SAIDA, :RECEBIDO, :PAGO, :NOMEBANCO, :N' +
        'UMCONTA)')
    DeleteSQL.Strings = (
      'delete from Saldo'
      'where'
      '  CODBANCO = :OLD_CODBANCO')
    Left = 272
    Top = 30
  end
